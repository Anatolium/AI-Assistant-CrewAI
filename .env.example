OPENAI_API_KEY="your_OPENAI_API_KEY"
OPENAI_API_BASE=https://api.openai.com/v1

# Отключает телеметрию, специфичную для CrewAI
CREWAI_DISABLE_TELEMETRY="true"

# Отключает весь OpenTelemetry SDK (который CrewAI использует для трассировки)
# Это является дополнительной мерой.
OTEL_SDK_DISABLED="true"
